{{ header }}{{ column_left }}
<div id="content">
  <div class="page-header">
    <div class="container-fluid">
      <div class="pull-right">
        <button type="button" id="button-setting" data-toggle="tooltip" title="{{ button_setting }}" data-loading-text="{{ text_loading }}" class="btn btn-info hidden"><i class="fa fa-cog"></i></button>
      </div>
      <h1><i class="icon icon-grid-big"></i> {{ heading_title }}</h1>
      <ul class="breadcrumb">
        {% for breadcrumb in breadcrumbs %}
          <li><a href="{{ breadcrumb.href }}">{{ breadcrumb.text }}</a></li>
        {% endfor %}
      </ul>
    </div>
  </div>
  <div class="container-fluid">
    {% if error_install %}
      <div class="alert alert-danger"><i class="fa fa-exclamation-circle"></i> {{ error_install }}
        <button type="button" class="close" data-dismiss="alert">&times;</button>
      </div>
    {% endif %}
    <div class="row">
      {{ total_orders_mini }}
      {{ total_sales_mini }}
      {{ total_customers_mini }}
      {{ users_online_mini }}
    </div>
    <div class="row">
      {{ store_sales }}
    </div>
    <div class="row">
      {{ chart_sales_analytics_large }}
    </div>
    <div class="row">
      {{ map_medium }}
      {{ activity_medium }}
    </div>
    <div class="row">
      <div class="col-md-6">
        <div class="row">
          {{ chart_most_viewed_products_medium }}
        </div>
      </div>
      <div class="col-md-6">
        <div class="row">
          {{ completed_orders_mini }}
          {{ returned_orders_mini }}
          {{ processing_orders_mini }}
          {{ product_views_mini }}
        </div>
      </div>
    </div>
    <div class="row">
      {{ chart_top_customer_medium }}
      {{ chart_top_product_medium }}
    </div>
    <div class="row">
      {{ latest_orders_large }}
    </div>
  </div>
</div>
{{ footer }}