{% set current_section = '' %}
{% if b5b_qore_engine_route%}
    {% set current_section = b5b_qore_engine_route|split('/') %}
{% endif %}

{% set is_homepage = FALSE %}

{% if current_section and current_section[0] == 'common' and current_section[1] == 'dashboard' %}
    {% set is_homepage = TRUE %}
{% endif %}

{% if is_homepage %}
    {% set class = 'homepage' %}
{% else %}
    {% set class = 'not-homepage' %}
{% endif %}

{% if b5b_qore_engine_color_preset %}
  {% if b5b_qore_engine_white_label is defined and b5b_qore_engine_white_label and b5b_qore_engine_white_label.enable_white_label and b5b_qore_engine_white_label.use_own_logo %}
    {% set logo = 'user_upload/logo-white-' ~ b5b_qore_engine_color_preset %}
  {% else %}
    {% set logo = 'logo-white-' ~ b5b_qore_engine_color_preset %}
  {% endif %}
{% else %}
  {% set logo = 'logo-white-mixed' %}
{% endif %}

{% if b5b_qore_engine_white_label is defined and b5b_qore_engine_white_label and b5b_qore_engine_white_label.enable_white_label %}
    {% if b5b_qore_engine_white_label.show_b5b_support_header %}
        {% set show_b5b_support_header = TRUE %}
    {% else %}
        {% set show_b5b_support_header = FALSE %}
    {% endif %}
{% else %}
    {% set show_b5b_support_header = TRUE %}
{% endif %}

